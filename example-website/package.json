{
  "name": "ansybl-example-website",
  "version": "1.0.0",
  "description": "Example website implementing the Ansybl social syndication protocol",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "build": "node build.js",
    "test": "node --test test/*.test.js",
    "deploy": "bash scripts/deploy.sh production",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "backup": "bash scripts/backup.sh",
    "restore": "bash scripts/restore.sh",
    "docker:build": "docker build -t ansybl-example:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "k8s:deploy": "kubectl apply -f kubernetes/ -n ansybl",
    "k8s:delete": "kubectl delete -f kubernetes/ -n ansybl"
  },
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "blurhash": "^2.0.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^3.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ffprobe-static": "^3.1.0",
    "helmet": "^7.1.0",
    "jsdom": "^23.2.0",
    "marked": "^9.1.6",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "music-metadata": "^11.9.0",
    "pdf-parse": "^2.4.5",
    "sharp": "^0.34.4",
    "socket.io": "^4.7.5",
    "uuid": "^13.0.0",
    "validator": "^13.15.22",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ansybl",
    "social-media",
    "decentralized",
    "syndication",
    "web"
  ],
  "author": "Ansybl Example",
  "license": "MIT"
}
